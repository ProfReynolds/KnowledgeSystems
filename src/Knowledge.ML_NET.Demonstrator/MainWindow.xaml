<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Knowledge.ML_NET.Demonstrator"
        xmlns:Digits="clr-namespace:Knowledge.ML_NET.Demonstrator.Digits" x:Name="Sasquatch" x:Class="Knowledge.ML_NET.Demonstrator.MainWindow"
        mc:Ignorable="d"
        Title="Microsoft ML.NET Demo by Prof Reynolds"
        Height="450" Width="900" 
        ResizeMode="NoResize" Loaded="Window_Loaded">
    <Canvas x:Name="MyCanvas">

        <Digits:DemoControl x:Name="InitialControlOccurance" Height="100" Width="100"
                            BorderThickness="2" BorderBrush="Blue" Background="Aquamarine" Canvas.Left="10" Canvas.Top="10"/>

    </Canvas>
</Window>
